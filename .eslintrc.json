{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]}
        ],
        "react/function-component-definition": 0,
        "arrow-body-style": 0,
        "react/prop-types": 0,
        "no-console": 0,
        "no-underscore-dangle": 0
    }
}
